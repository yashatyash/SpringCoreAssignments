<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="circle" class="com.yash.springcore.assignments.Ques14.ShapeFactory"
          factory-method="getCircleObject">
    </bean>

    <bean id="square" class="com.yash.springcore.assignments.Ques14.ShapeFactory"
          factory-method="getSquareObject">
    </bean>

    <bean id="rectangle" class="com.yash.springcore.assignments.Ques14.ShapeFactory"
          factory-method="getRectangleObject">
    </bean>

</beans>